<Window x:Class="CefSharp.MinimalExample.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:main="clr-namespace:CefSharp.MinimalExample.Wpf.Views"
        Title="{Binding WebBrowser.Title}"
        WindowState="Maximized">
    <main:TabsContainer x:Name="tabControl">
        <main:TabPage Header="Google">
            <main:BrowseTab />
        </main:TabPage>
        <main:TabPage Header="Github">
            <main:BrowseTab />
        </main:TabPage>
        <main:TabPage Header="Youtube">
            <main:BrowseTab />
        </main:TabPage>
    </main:TabsContainer>
</Window>
